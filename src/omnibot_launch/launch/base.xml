<launch>
    <param name="robot_description" command="cat $(find omnibot_base)/urdf/base.urdf" />
	<node name="omnibot_differential_base" pkg="omnibot_base" type="omnibot_differential_base" />
    <node name="omnibot_enable" pkg="omnibot_base" type="omnibot_enable" />

    <node name="scan_to_base_pub" pkg="tf" type="static_transform_publisher" args="0 0 0.5 3.14 0 0 base_link laser 100" />
</launch>
